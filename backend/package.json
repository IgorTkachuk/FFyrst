{
  "private": true,
  "engines": {
    "node": "14.x.x",
    "npm": "6.x.x"
  },
  "scripts": {
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "start:dev": "nodemon --exec ts-node -r dotenv/config -r tsconfig-paths/register ./src/server.ts",
    "start": "node -r dotenv/config ./src/server.js",
    "build:ts": "tsc && tscpaths -p tsconfig.json --src . --out build",
    "build:copy": "cp -r shared build && cp -r public build",
    "build": "npm run build:ts && npm run build:copy"
  },
  "dependencies": {
    "@types/nodemailer": "^6.4.2",
    "nodemailer": "^6.6.2",
    "bcrypt": "5.0.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "i": "^0.3.6",
    "jsonwebtoken": "8.5.1",
    "nanoid": "3.1.20",
    "npm": "^7.19.1",
    "nodemailer": "^6.6.2",
    "pg": "8.5.1",
    "pino": "6.11.1",
    "pino-pretty": "4.5.0",
    "pug": "^3.0.2",
    "pug-cli": "^1.0.0-alpha6",
    "sequelize": "6.5.0",
    "shared": "file:shared",
    "ts-node": "9.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.11",
    "@types/jsonwebtoken": "8.5.3",
    "@types/node": "14.14.31",
    "@types/pino": "6.3.6",
    "@types/sequelize": "4.28.9",
    "nodemon": "2.0.7",
    "sequelize-cli": "6.2.0",
    "tsconfig-paths": "3.9.0",
    "tscpaths": "0.0.9",
    "typescript": "4.1.2"
  }
}
